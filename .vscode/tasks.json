{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: arduino-cli compile",
			"command": "arduino-cli",
			"args": [
				"compile",
				"-v",
				"-b",
				"arduino:avr:mini",
				"${file}"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "compiler: arduino-cli"
		}
	]
}